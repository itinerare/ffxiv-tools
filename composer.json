{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "erusev/parsedown": "^1.7",
        "guzzlehttp/guzzle": "^7.2",
        "laracasts/flash": "^3.2",
        "laravel/framework": "^12.0",
        "laravel/tinker": "^2.7",
        "nesbot/carbon": "^3.7",
        "spatie/laravel-html": "^3.5",
        "xivapi/lodestone-parser": "dev-main"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://code.itinerare.net/internal/lodestone-parser"
        }
    ],
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.0.1",
        "marcocesarato/php-conventional-changelog": "^1.17",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^8.1",
        "phpunit/phpunit": "^11.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "git describe --tags > version && git log -n1 --pretty=%ci HEAD >> version"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "sniff": [
            "./vendor/bin/pint --test"
        ],
        "lint": [
            "./vendor/bin/pint"
        ],
        "changelog": [
            "./vendor/marcocesarato/php-conventional-changelog/conventional-changelog"
        ],
        "release": [
            "./vendor/marcocesarato/php-conventional-changelog/conventional-changelog --commit"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "version": "2.22.0"
}